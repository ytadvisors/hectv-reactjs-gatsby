{
  "name": "hectv-react-gatsby",
  "description": "HEC-TV website",
  "license": "MIT",
  "scripts": {
    "build:dev": "ACTIVE_ENV=development  node --max_old_space_size=4084  --inspect-brk --no-lazy  ./node_modules/.bin/gatsby build --prefix-paths --verbose --no-uglify",
    "build:staging": "ACTIVE_ENV=staging node --max_old_space_size=12000  --stack-size=100000 ./node_modules/.bin/gatsby build --prefix-paths --verbose --no-uglify",
    "build:production": "ACTIVE_ENV=production node  --max_old_space_size=12000  --stack-size=100000 ./node_modules/.bin/gatsby build --prefix-paths --verbose",
    "sync-static:staging": "aws --profile eb-cli2 s3 sync --exclude '*' --include '/*.js' --include '/*.css' --include '/*.json' public/ s3://dev.hecmedia.org/",
    "sync-dynamic:staging": "aws --profile eb-cli2 s3 sync public/ s3://dev.hecmedia.org --cache-control 'max-age=0'",
    "sync-static:production": "aws --profile eb-cli2 s3 sync --exclude '*' --include '/*.js' --include '/*.css' --include '/*.json' public/ s3://hectv.org/",
    "sync-dynamic:production": "aws --profile eb-cli2 s3 sync public/ s3://hectv.org --cache-control 'max-age=0'",
    "clear:production": "aws cloudfront create-invalidation --distribution-id E3MZJ4ROMYBGNG --paths '/*'",
    "clear:staging": "aws cloudfront create-invalidation --distribution-id E3DVN24AGRZ5KG --paths '/*' ",
    "dev": " node --max_old_space_size=2084 --inspect-brk --no-lazy ./node_modules/.bin/gatsby develop",
    "develop": " node --max_old_space_size=8084 ./node_modules/.bin/gatsby develop",
    "build": " node --max_old_space_size=2084 ./node_modules/.bin/gatsby build",
    "serve": " node --max_old_space_size=2084 ./node_modules/.bin/gatsby serve"
  },
  "husky": {
    "hooks": {
      "pre-commit": "NODE_ENV=production lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,css}": [
      "pretty-quick --staged",
      "eslint src/ --fix ",
      "git add"
    ]
  },
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "aws-sdk": "2.349.0",
    "axios": "0.18.0",
    "bluebird": "3.5.1",
    "bootstrap": "^3.3.7",
    "deep-map-keys": "1.2.0",
    "dotenv": "6.0.0",
    "dotenv-webpack": "1.5.7",
    "eslint-plugin-react": "7.11.1",
    "gatsby": "2.0.22",
    "gatsby-plugin-google-fonts": "0.0.4",
    "gatsby-plugin-google-tagmanager": "1.0.19",
    "gatsby-plugin-mailchimp": "2.2.3",
    "gatsby-plugin-offline": "2.0.5",
    "gatsby-plugin-react-helmet": "3.0.0",
    "gatsby-plugin-react-next": "next",
    "gatsby-plugin-robots-txt": "1.3.0",
    "gatsby-plugin-sass": "2.0.1",
    "gatsby-plugin-sitemap": "2.0.1",
    "gatsby-source-wordpress": "3.0.13",
    "google-fonts": "1.0.0",
    "graphql": "0.13.2",
    "install": "0.12.1",
    "jquery": "3.3.1",
    "lodash": "4.17.10",
    "moment": "2.22.2",
    "node-sass": "4.9.2",
    "prop-types": "15.6.2",
    "query-string": "6.1.0",
    "react": "16.5.2",
    "react-bootstrap": "0.32.1",
    "react-datepicker": "1.5.0",
    "react-dates": "17.0.0",
    "react-dom": "16.4.2",
    "react-google-maps": "9.4.5",
    "react-helmet": "5.2.0",
    "react-icons": "2.2.7",
    "react-lazyload": "2.3.0",
    "react-player": "1.6.4",
    "react-recaptcha": "2.3.10",
    "react-redux": "5.0.7",
    "react-redux-form": "1.16.9",
    "react-redux-loading-bar": "4.0.5",
    "react-router-dom": "4.3.1",
    "react-select": "2.1.1",
    "react-share": "2.3.1",
    "react-stripe-elements": "2.0.0",
    "react-transition-group": "2.4.0",
    "redux": "4.0.0",
    "redux-devtools-extension": "2.13.5",
    "redux-form": "7.4.2",
    "redux-logger": "3.0.6",
    "redux-saga": "0.16.0",
    "slick-carousel": "1.8.1",
    "validator": "10.4.0"
  },
  "devDependencies": {
    "@babel/core": "7.0.0",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.55",
    "@babel/plugin-proposal-optional-chaining": "7.0.0-beta.55",
    "@babel/plugin-transform-runtime": "7.0.0-beta.55",
    "@babel/preset-env": "7.0.0-beta.55",
    "@babel/preset-flow": "7.0.0-beta.55",
    "@babel/preset-react": "7.0.0-beta.55",
    "babel-eslint": "8",
    "eslint": ">=5",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prettier": "3.0.0",
    "husky": "1.1.2",
    "lint-staged": "7.3.0",
    "prettier": "1.14.3",
    "pretty-quick": "1.8.0"
  }
}
